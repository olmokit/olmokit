  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:olmokit/olmokit.git"
  },
  "type": "module",
  "scripts": {
    "dev": "ts-node tools/dev.ts",
    "dev-20": "node --loader ts-node/esm tools/dev.ts",
    "start": "ts-node tools/dev.ts -- start",
    "start-20": "node --loader ts-node/esm tools/dev.ts -- start",
    "build": "ts-node tools/dev.ts -- build",
    "build-20": "node --loader ts-node/esm tools/dev.ts -- build",
    "lint": "nx run-many --targets=lint --all",
    "postinstall": "husky install && pnpm dev postinstall",
    "format": "prettier **/*.{js,json,ts,tsx,css,scss,md,yml,yaml,php} --write",
    "depsupdate": "pnpm update --recursive --latest --filter=!./apps/docs --interactive",
    "depsupdate-ni": "pnpm update --recursive --latest --filter=!./apps/docs",
    "docs": "nx run docs:watch"
  },
  "lint-staged": {
    "**/*.{js,json,ts,tsx,css,scss,md,yml,yaml,php}": "prettier --write"
  },
