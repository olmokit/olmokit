{
  "MarkdownPaste.path": "${workspaceRoot}/apps/docs/static/img/screenshots",
  "files.exclude": {
    ".docusaurus": true,
    ".github": true,
    ".husky": true,
    ".vscode": true,
    "dist": true,
    "tmp": true,
    "package-lock.json": true,
    "pnpm-lock.yaml": true,
    // these are autogenerated
    "apps/docs/.docusaurus": true,
    "apps/docs/build": true,
    "apps/docs/docs/libs": true
  },
  "window.title": "${dirty} @olmokit/olmokit - ${activeFolderMedium}",
  "workbench.colorCustomizations": {
    "activityBar.background": "#2e1f2b",
    "activityBar.foreground": "#FF52DE"
  },
  "files.associations": {
    "*.json__tpl__": "json",
    "*.js__tpl__": "javascript",
    "*.ts__tpl__": "typescript",
    "*.md__tpl__": "markdown",
    "env*__tpl__": "properties",
    ".htaccess*": "ruby",
    "gitignore": "ignore"
  },
  "todohighlight.keywords": [
    {
      "text": "DEBUG:",
      "color": "white",
      "backgroundColor": "#726b11"
    },
    {
      "text": "FLAG:",
      "color": "white",
      "backgroundColor": "#8f6d60"
    },
    {
      "text": "EXP:",
      "color": "white",
      "backgroundColor": "#96007d"
    },
    {
      "text": "NOTE:",
      "color": "white",
      "backgroundColor": "blue"
    }
  ],
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "README.md": "authors, citation*, codeowners, contributors, copying, credits, license*, maintainers, appveyor*, azure-pipelines*, crowdin*, dangerfile*, lint-staged*, pullapprove*, renovate*, stylelint*, tsdoc.*, tslint*, turbo*, typedoc*, vercel*, index.html, jasmine.*, tsdoc.*, pnpm*, yarn*, env-*, env.*, .*, *Dockerfile, *.toml, *.lock, *.preset.ts, *.preset.js, *.config.ts, *.config.js, *.config.cjs, *.config.mjs, *.md, *.txt, *.json"
  }
}
